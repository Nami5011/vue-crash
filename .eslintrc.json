{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:vue/vue3-essential",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        // "ecmaVersion":"latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
		"vue/multi-word-component-names": 0,
		"vue/no-reserved-component-names": 0
    }
}
